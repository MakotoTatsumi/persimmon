{
  "name": "next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
    "fix": "run-p fix:*",
    "fix:ts": "eslint -c \"./src/**/*.{ts,tsx}\" --fix",
    "fix:config": "prettier ./.eslintrc.js ./*.js --write",
