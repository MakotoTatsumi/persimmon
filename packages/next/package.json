    "fix:json": "prettier ./*.json ./**/*.json --write"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/react": "17.0.31",
    "eslint": "8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-unicorn": "^37.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "stylelint": "^14.0.1",
    "typescript": "4.4.4"
  }
}
