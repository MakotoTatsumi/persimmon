{
  "name": "next",
  "scripts": {
    "build": "next build",
    "check-all": "npm-run-all -p spell-check type-check lint -s test",
    "compare-story": "run-s -c ss:capture reg-suit:save reg-suit:open",
    "dev": "next dev",
    "fix": "run-p fix:*",
    "fix-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "fix-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --write",
    "fix:config": "prettier ./.eslintrc.js ./*.js --write",
    "fix:json": "prettier ./*.json ./**/*.json --write",
    "fix:pack": "fixpack",
    "fix:ts": "run-s fix-ts-by-eslint fix-ts-by-prettier",
    "lint": "run-p lint:*",
    "lint-ts-by-eslint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint-ts-by-prettier": "prettier \"./src/**/*.{ts,tsx}\" --check",
    "lint:css": "stylelint \"./src/**/*.{ts,tsx}\"",
    "lint:ts": "run-s lint-ts-by-eslint lint-ts-by-prettier",
    "reg-suit:open": "open-cli \"./report.html\"",
    "reg-suit:save": "reg-cli \"./.screenshot/actual\" \"./.screenshot/expected\" \"./.screenshot/diff\" -R \"./report.html\"",
    "spell-check": "cspell \"./src/**/*.{ts,tsx}\"",
    "ss:capture": "storycap http://localhost:6006 -o \".screenshot/actual\"",
    "ss:copy": "shx cp -r \"./.screenshot/actual/*\" \"./.screenshot/expected\"",
    "ss:git-add": "git add \"./.screenshot/expected\"",
    "ss:git-commit": "git commit -m \"docs: update expected screenshot@`date \"+%Y-%m-%d %H:%M:%S\"`\"",
    "ss:remove": "shx rm -rf \"./.screenshot/expected/*\"",
    "start": "next start",
    "storybook": "start-storybook -p 6006 -s public",
    "test": "jest",
    "type-check": "tsc",
    "update-ss": "run-s ss:remove ss:copy ss:git-add ss:git-commit"
  },
  "dependencies": {
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@storybook/addon-viewport": "^6.4.9",
    "@storybook/react": "6.4.0-beta.8",
    "@storybook/testing-react": "1.0.0-next.0",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@swc/core": "^1.2.117",
    "@swc/jest": "^0.2.11",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/react": "17.0.31",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.15",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "babel-jest": "^27.4.2",
    "cspell": "^5.13.1",
    "eslint": "8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-unicorn": "^37.0.1",
    "fixpack": "^4.0.0",
    "jest": "^27.4.3",
    "known-css-properties": "^0.23.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss-jsx": "^0.36.4",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "reg-keygen-git-hash-plugin": "^0.11.1",
    "reg-suit": "^0.11.1",
    "shx": "^0.3.3",
    "storycap": "^3.1.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.4.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "4.4.4"
  },
  "version": "0.1.0",
  "private": true
}
